@model IEnumerable<ShenmiApp.Models.Niveles>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreNivel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PuntajeMinimo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PuntajeMaximo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Completado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PreguntasIdPreguntas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MundosIdMundosNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreNivel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PuntajeMinimo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PuntajeMaximo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Completado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PreguntasIdPreguntas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MundosIdMundosNavigation.IdMundos)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdNiveles">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdNiveles">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdNiveles">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
